{
    "name": "chatwiki/yii2",
    "require": {
        "php": "^8.3",
        "yiisoft/yii2": "~2.0.0",
        "spiral/roadrunner-worker": "^3.6",
        "spiral/goridge": "^4.2",
        "yiisoft/yii2-httpclient": "^2.0",
      "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.10",
        "nesbot/carbon": "^3.10",
        "league/html-to-markdown": "^5.1"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        }
    }
}
